<?xml version="1.0" encoding="UTF-8"?>
<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
    <name>NobleToolsTypeSystemDescriptor</name>
    <description/>
    <version>1.0</version>
    <vendor/>
  <types>
    <typeDescription>
      <name>edu.pitt.dbmi.nlp.noble.uima.types.Concept</name>
      <description>Represents  a NobleTools Concept.</description>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>cui</name>
          <description>Concept unique identifier.</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>tuis</name>
          <description>Set of Semantic Types</description>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>edu.pitt.dbmi.nlp.noble.uima.types.SemanticType</elementType>
        </featureDescription>
        <featureDescription>
          <name>synonyms</name>
          <description>Set of synonyms.</description>
          <rangeTypeName>uima.cas.StringArray</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>cn</name>
          <description>Concept Name</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>preferredTerm</name>
          <description>Preferred Term</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>definition</name>
          <description>Definition.</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>edu.pitt.dbmi.nlp.noble.uima.types.SemanticType</name>
      <description>Noble Tools Semantic Type</description>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>tui</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>sty</name>
          <description>Semantic Type Name</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>edu.pitt.dbmi.nlp.noble.uima.types.Sentence</name>
      <description>Noble Coder Sentence</description>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>numberOfCharacters</name>
          <description>Number of characters underlying this sentence.</description>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>underLyingText</name>
          <description>Text beneath sentence.</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
  </types>
</typeSystemDescription>
