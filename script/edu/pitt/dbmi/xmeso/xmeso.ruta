//XmesoCodeAssigner{->CALL(Ie.ExerciseTumorFormAssignment)};
XmesoTumorForm{->CALL(Ie.AssignTumorSite)};
XmesoTumorForm{->CALL(Ie.AssignTumorConfiguration)};
XmesoTumorForm{->CALL(Ie.AssignTumorDifferentiation)};
XmesoTumorForm{->CALL(Ie.AssignTumorSize)};
XmesoEncounterForm{->CALL(Ie.AssignSurgicalProcedure)};
XmesoEncounterForm{->CALL(Ie.AssignHistologicalType)};



