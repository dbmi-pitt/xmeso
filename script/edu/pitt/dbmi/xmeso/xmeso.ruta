Document{->CALL(Sectionizer.SectionDetector)};
Document{->CALL(Sectionizer.PartHeaderAnnotator)};
PartHeader{->CALL(FloatNumberDetector.CleanBeneath)};
Section{->CALL(Sectionizer.PartHeaderSectionNameAssigner)};
Document{->CALL(Sectionizer.PartDetector)};

Part{->CALL(Ner.NameEntityHistologicalType)};
Part{->CALL(Ner.NameEntityImmunohistochemicalProfile)};
Part{->CALL(Ner.NameEntityTumorSite)};
Part{->CALL(Ner.NameEntitySurgicalProcedure)};
Part{->CALL(Ner.NameEntityTumorConfiguration)};
Part{->CALL(Ner.NameEntityTumorDifferentiation)};
XmesoNamedEntity{->CALL(FloatNumberDetector.CleanBeneath)};
Part{->CALL(Ner.AssignSectionsAndPartsToNamedEntities)};

//
// Vamsi,
//
//   Here is where you can integrete to the NegEx Detector
//
Document{-> EXEC(NegExAnnotationEngine, {XmesoNamedEntity})};

//
